{
  "name": "family-tree-api",
  "version": "1.0.0",
  "description": "API for family trees",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index",
    "dev": "nodemon src/index",
    "db": "docker run -d -p 54321:5432 -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=postgres --rm --name db postgres:9-alpine",
    "psql": "docker container exec -it db psql -U postgres",
    "docker:build": "docker build . -t family-tree-api",
    "docker:up": "docker compose up -d"
  },
  "keywords": [],
  "author": "Luca Di Fazio",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^2.0.14",
    "sequelize-cli": "^6.2.0"
  },
  "dependencies": {
    "apollo-server": "^3.4.0",
    "graphql": "^15.7.2",
    "pg": "^8.7.1",
    "sequelize": "^6.8.0"
  }
}
